package listas;

import java.util.ArrayList;
import java.util.List;

public class Exemplo1 {

	public static void main(String[] args) {
		
		List<String> nomes = new ArrayList<>();
		nomes.add("Adriano");
		nomes.add("Rebeca");
		nomes.add("Jr");
		System.out.println(nomes);
		nomes.remove(2);

	}

}
